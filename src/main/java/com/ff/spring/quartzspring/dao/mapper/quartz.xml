<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ff.spring.quartzspring.dao.QuartzDao">




    <select id="loadById" resultType="Quartz" parameterType="String">

        SELECT  *
        FROM quartz
        where quartz.QUARTZ_ID=#{id}

    </select>



</mapper>